<launch>
	<param name="robot_description" command="$(find xacro)/xacro.py $(find p2os_urdf)/defs/pioneer3at.xacro" />
    
  <node pkg="robot_state_publisher" type="state_publisher" name="robot_state_publisher">
    <remap from="joint_state" to="joint_state"/>
    <param name="publish_frequency" type="double" value="10.0"/>
    <param name="tf_prefix" type="string" value=""/>
  </node>
  
	<node pkg="p2os_urdf" type="publisher" name="publisher">
    <param name="publish_odom"  type="bool" value="false"/>
    <param name="publish_sonar" type="bool" value="false"/>
  </node>
</launch>
